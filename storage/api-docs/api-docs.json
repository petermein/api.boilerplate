{
    "openapi": "3.0.0",
    "info": {
        "title": "Boilerplate API",
        "contact": {
            "email": "peter@infratron.io"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api/v1"
        },
        {
            "url": "http://localhost:8000/api/v2"
        }
    ],
    "paths": {
        "/api/v1/example": {
            "get": {
                "summary": "list examples",
                "operationId": "Boilerplate\\UI\\Api\\REST\\Example\\ExampleController::getAll",
                "responses": {
                    "200": {
                        "description": "A list with examples"
                    },
                    "default": {
                        "description": "an \"unexpected\" error"
                    }
                }
            }
        }
    }
}