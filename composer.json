{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4",
    "babenkoivan/elastic-migrations": "^1.2",
    "darkaonline/swagger-lume": "7.*",
    "doctrine/inflector": "^1.4",
    "itsgoingd/clockwork": "^4.1",
    "laravel-doctrine/orm": "^1.6",
    "laravel/lumen-framework": "^7.0",
    "rebing/graphql-laravel": "^5.1",
    "spatie/data-transfer-object": "^2.3",
    "thecodingmachine/safe": "^1.1"
  },
  "require-dev": {
    "ergebnis/phpstan-rules": "^0.15.1",
    "friendsofphp/php-cs-fixer": "^2.16",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/larastan": "^0.6.2",
    "phpstan/phpstan": "^0.12.37",
    "phpstan/phpstan-deprecation-rules": "^0.12.5",
    "phpstan/phpstan-strict-rules": "^0.12.4",
    "phpunit/phpunit": "^8.5",
    "rector/rector": "^0.7",
    "sensiolabs-de/deptrac-shim": "^0.8.2",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/messenger": "^5.0",
    "thecodingmachine/phpstan-safe-rule": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Api\\": "src/"
    },
    "files": [
      "src/Common/Object.php",
      "src/Common/Helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
