{
    "name": "petermein/api.boilerplate",
    "description": "A CQRS domain driven design template for API's",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "Closed",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "doctrine/inflector": "^1.4",
        "illuminate/redis": "^8.0",
        "itsgoingd/clockwork": "^4.1",
        "laravel-doctrine/orm": "^1.7",
        "laravel/lumen-framework": "^8.0",
        "myclabs/php-enum": "^1.7",
        "rebing/graphql-laravel": "^5.1",
        "spatie/data-transfer-object": "^2.3",
        "thecodingmachine/safe": "^1.1"
    },
    "require-dev": {
        "cebe/php-openapi": "^1.4",
        "doctrine/annotations": "^1.10",
        "ergebnis/phpstan-rules": "^0.15.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/larastan": "^0.6.2",
        "overtrue/phplint": "^3.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpdocumentor/type-resolver": "^1.4",
        "phpstan/phpstan": "^0.12.37",
        "phpstan/phpstan-deprecation-rules": "^0.12.5",
        "phpstan/phpstan-strict-rules": "^0.12.4",
        "phpunit/phpunit": "^8.5",
        "qossmic/deptrac-shim": "^0.15.2",
        "rector/rector": "^0.11",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/messenger": "^5.0",
        "thecodingmachine/phpstan-safe-rule": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "src/"
        },
        "files": [
            "src/Common/Object.php",
            "src/Common/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
